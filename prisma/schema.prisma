generator client_mysql {
  output = "@prisma/client/mysql"
  provider = "prisma-client-js"
}


datasource mysql {
  provider = "mysql"
  url      = env("DATABASE_URL")
}


model Pad {
  id        Int      @id @default(autoincrement())
  name      String
}
